    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "^2.0.2",
    "path": "0.12.7",
    "style-loader": "^1.1.3",
