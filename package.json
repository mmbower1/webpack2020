{
  "name": "webpack2020",
  "version": "1.0.0",
  "description": "webpack2020 template",
  "main": "server/server.js",
  "scripts": {
    "build": "babel-node ./node_modules/webpack/bin/webpack",
    "start": "babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server --open nodemon server/server.js",
    "server": "nodemon server/server.js",
    "src": "cd src && npm start",
    "dev": "concurrently \"nodemon server.js\" \"npm run src\""
  },
